@model Project



<tr>
    <td>@Model.Bookid</td>
    <td>@Model.Title</td>
    <td>@Model.Author_FirstName</td>
    <td>@Model.Author_MiddleName</td>
    <td>@Model.Author_LastName</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td>$@Model.Price</td>
    <td>@Model.Pages</td>
</tr>